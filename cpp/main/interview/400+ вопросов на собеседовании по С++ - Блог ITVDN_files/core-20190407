(function () {
    

    function resultWhileSuccess(form) {
        $(form).find(".subscription-email")
            .find(".subscription-submit").hide();
    };

    function newsletterSubscriptionSubmit(event) {
        event.preventDefault();


        var email = $(this).find(".email-input").val(),
            url = $(this).data("url"),
            loader = $(this).find(".form-preloader"),
            submitButton = $(this).find(".subscription-submit"),
            form = this;

        if ($("#newsletter-subscription-form").valid()) {
            loader.show();
            $.ajax({
                url: url,
                data: {
                    "group": "ITVDN",
                    "email": email
                }
            }).done(function (result) {
                loader.hide();
                resultWhileSuccess(form);
            }).fail(function (error) {
                loader.hide();
                resultWhileSuccess(form);
            })
        }
    };

    $(document).ready(function () {
        $("#newsletter-subscription-form").submit(newsletterSubscriptionSubmit);
    });

})();;
$(document).ready(function () {
    const wasCookieMessageShown = localStorage.getItem("wasCookieMessageShown")
    setTimeout(() => cookieMessageRun(), 2000)

    function cookieMessageRun() {
        if (!wasCookieMessageShown) {
            showHideCookieMessage("show")
            const btnToAccept = $(".cookies-message__accept")
            $(btnToAccept).on('click', function () {
                showHideCookieMessage("hide")
            })
        }
    }

    function showHideCookieMessage(flag) {
        const cookieMessage = $(".cookies-message")
        if (flag === "show") {
            cookieMessage.css('display', 'block')
        } else if (flag === "hide") {
            cookieMessage.css('display', 'none')
            localStorage.setItem("wasCookieMessageShown", true)
        }
    }
});
/*
    jQuery Masked Input Plugin
    Copyright (c) 2007 - 2015 Josh Bush (digitalbush.com)
    Licensed under the MIT license (http://digitalbush.com/projects/masked-input-plugin/#license)
    Version: 1.4.1
*/

