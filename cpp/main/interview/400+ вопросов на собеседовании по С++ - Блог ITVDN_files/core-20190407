$.fn.setCursorPosition = function (pos) {
    if ($(this).get(0).setSelectionRange) {
        $(this).get(0).setSelectionRange(pos, pos);
    } else if ($(this).get(0).createTextRange) {
        var range = $(this).get(0).createTextRange();
        range.collapse(true);
        range.moveEnd('character', pos);
        range.moveStart('character', pos);
        range.select();
    }
};;
$(document).ready(function () {
    const http = ITVDN.http;
    const storageKey = 'sub-exp-pop-up-' + window.userName;
    const localizationScope = '~/js/subscription-message.js';
    var errorHappend = false;
    var popUpData = null;

    function init() {
        if (window.userIsAuthenticated !== 'True') {
            return;
        }

        popUpData = getPopUpData();

        if (popUpData && new Date() <= new Date(popUpData.expire)) {
            loadAndInitPopUp();
            return;
        }

        http.get('/api/subscription-expiration/show-pop-up')
            .then((res) => {
                popUpData = {};
                popUpData.code = '';
                popUpData.closed = res === true ? false : true;
                popUpData.expire = new Date().addMinutes(360).getTime();

                savePopUpData()
                loadAndInitPopUp()
            });
    }

    function getPopUpData() {
        var storageData = localStorage.getItem(storageKey);
        return storageData ? JSON.parse(storageData) : null
    }

    function savePopUpData() {
        localStorage.setItem(storageKey, JSON.stringify(popUpData));
    }

    function loadAndInitPopUp() {
        if (popUpData.closed) {
            return;
        }

        http.get('/' + localization.cultureUrl + '/templates/subscription-message')
            .then(initPopUp);
    }

    function markPopUpAsClosed() {
        if (!errorHappend) {
            popUpData.closed = true;
            savePopUpData();
        }
    }

    function initPopUp(template) {
        $('body').append(template);

        var modal = $('.subscription-modal'),
            firstContainer = modal.find('.subscription-modal__first-step'),
            secondContainer = modal.find('.subscription-modal__second-step'),
            loader = modal.find('.loader-wrapper'),
            promoBlock = modal.find('.subscription-modal__promo-block'),
            promo = promoBlock.find('.subscription-modal__promo'),
            clipboard = promoBlock.find('.subscription-modal__icon');

        firstContainer.show();
        secondContainer.hide();

        firstContainer.find('.btn-filled-orange').click(function (e) {
            function showErrorMessage(message) {
                errorHappend = true;
                secondContainer.find('p:first').text(message);
                secondContainer.find('p:first').css('color', 'red');
                secondContainer.find('.btn-not-filled').css('margin', 'auto');
                secondContainer.find('.btn-filled-orange').remove();
                promoBlock.remove();
                loader.hide();
                secondContainer.show();
            }

            firstContainer.hide();

            const updatedPopUpData = getPopUpData();

            if (updatedPopUpData.code != popUpData.code) {
                popUpData.code = updatedPopUpData.code;
            }

            if (popUpData.code) {
                promo.html(popUpData.code);
                secondContainer.show();

                return;
            }

            loader.show();

            http.get('/api/subscription-expiration/promo')
                .then(response => {
                    let successed = response.successed,
                        code = response.code,
                        message = response.message;

                    if (successed === true && code) {
                        promo.html(code);
                        loader.hide();
                        secondContainer.show();

                        popUpData.code = code;
                        savePopUpData();
                        markPopUpAsClosed();
                    }
                    else if (successed === false && message) {
                        showErrorMessage(message);
                        markPopUpAsClosed();
                    }
                    else {
                        showErrorMessage($.R(localizationScope, 'Error', 'Что-то пошло не так'));
                    }
                })
                .catch(() => {
                    showErrorMessage($.R(localizationScope, 'Error', 'Что-то пошло не так'));
                });
        });

        secondContainer.find('.btn-filled-orange').click(function (e) {
            markPopUpAsClosed();
            location.href = '/' + localization.cultureUrl + '/subscriptions';
        });

        firstContainer.find('.btn-not-filled').click((e) => {
            markPopUpAsClosed();
            modal.remove();

            http.get('/api/subscription-expiration/mark-accesses')
        });

        secondContainer.find('.btn-not-filled').click((e) => {
            markPopUpAsClosed();
            modal.remove();
        });

        clipboard.click(() => {
            copyToClipboard(promo);
            clipboard.css('color', '#f4520b');
        });

        modal.show();
    }

    function copyToClipboard(element) {
        let $temp = $("<input>");
        $("body").append($temp);
        $temp = $temp.val($(element).text()).select();
        document.execCommand("copy");
        $temp.remove();
    }

    init();
});;
